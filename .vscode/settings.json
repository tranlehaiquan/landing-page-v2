{
    "files.associations": {
        "*.css": "tailwindcss"
    },
    "tailwindCSS.classAttributes": ["class", "className", "theme"],
    "tailwindCSS.experimental.classRegex": [
        ["twMerge\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"],
        [
            "createTheme(?:<\\w+>)?\\s*\\(([^)]*)\\)",
            "{?\\s?[\\w].*:\\s*?[\"'`]([^\"'`]*).*?,?\\s?}?"
        ]
    ],
    "typescript.tsdk": "node_modules/typescript/lib",
    "i18n-ally.localesPaths": [
        "i18n",
        "messages",
        "components/locales",
        "lib/i18n",
        "webapp/src/backend/reducers/locales"
    ],
    "cSpell.words": ["supabase"]
}
